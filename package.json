{
  "name": "avoid-snakes-monorepo",
  "private": true,
  "type": "module",
  "packageManager": "pnpm@9.6.0",
  "scripts": {
    "dev:web": "http-server web -p 5173 -c-1 -a 127.0.0.1",
    "build:pipelines": "pnpm -r --filter './packages/*' --filter './data-pipelines' run build",
    "data:assets:ranges": "node data-pipelines/dist/cli/assets-ranges.js",
    "data:venommaps:pull": "node data-pipelines/dist/cli/venommaps-pull.js",
    "data:normalize": "node data-pipelines/dist/cli/occurrence-normalize.js",
    "data:enrich": "node data-pipelines/dist/cli/occurrence-enrich.js",
    "data:strict": "node data-pipelines/dist/cli/occurrence-strict.js",
    "data:heat:all": "node data-pipelines/dist/cli/occurrence-heat-all.js",
    "data:snakes": "node data-pipelines/dist/cli/run-all.js"
  },
  "devDependencies": {
    "http-server": "^14.1.1",
    "typescript": "^5.9.2"
  }
}
